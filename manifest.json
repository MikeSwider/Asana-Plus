{
  "background": {
    "scripts": [
      "dist/background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "dist/index.css"
      ],
      "js": [
        "dist/index.js"
      ],
      "matches": [
        "https://app.asana.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Enhance Asana with extra features: upload images by pasting, desktop notifications, shortcuts & more.",
  "icons": {
    "128": "/icons/128x128.png",
    "48": "/icons/48x48.png",
    "96": "/icons/96x96.png"
  },
  "manifest_version": 2,
  "name": "Asana Plus",
  "page_action": {
    "default_icon": {
      "19": "/icons/default/19x19.png",
      "38": "/icons/default/38x38.png"
    },
    "default_title": "Asana Plus"
  },
  "permissions": [
    "tabs",
    "https://app.asana.com/*",
    "notifications",
    "alarms",
    "storage"
  ],
  "short_name": "Asana Plus",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.12.0",
  "web_accessible_resources": [
    "dist/asana-plus.js",
    "dist/asana-plus-notifications.js",
    "door.mp3"
  ]
}
